{
  "name": "@greguintow/nest-fgql",
  "version": "1.0.10",
  "description": "Fast, lightweight GraphQL module to be used in NestJS",
  "author": "Mirco Bartels",
  "license": "MIT",
  "keywords": [
    "nest",
    "nestjs",
    "graphql",
    "graphql-jit",
    "fastify",
    "fastify-gql"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "format": "prettier **/**/*.ts --ignore-path ./.prettierignore --write",
    "lint": "eslint 'lib/**/*.ts' --fix",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "test:integration": "jest --runInBand",
    "test:integration:dev": "jest --runInBand --watch",
    "prerelease": "npm run build",
    "release": "release-it"
  },
  "devDependencies": {
    "@commitlint/cli": "16.0.2",
    "@commitlint/config-angular": "16.0.0",
    "@nestjs/common": "8.2.4",
    "@nestjs/core": "8.2.4",
    "@nestjs/platform-fastify": "8.2.4",
    "@nestjs/testing": "8.2.4",
    "@types/jest": "27.4.0",
    "@types/lodash": "^4.14.178",
    "@types/node": "17.0.8",
    "@types/node-fetch": "3.0.3",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "5.9.0",
    "@typescript-eslint/parser": "5.9.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "eslint": "8.6.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "lint-staged": "12.1.7",
    "prettier": "2.5.1",
    "reflect-metadata": "0.1.13",
    "release-it": "14.11.8",
    "rimraf": "3.0.2",
    "supertest": "6.1.6",
    "ts-jest": "27.1.2",
    "ts-morph": "^13.0.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  },
  "dependencies": {
    "@graphql-tools/merge": "6.2.5",
    "@graphql-tools/schema": "7.1.5",
    "@graphql-tools/utils": "7.10.0",
    "@nestjs/mapped-types": "1.0.0",
    "chokidar": "3.5.2",
    "fastify-gql": "npm:fastify-gql-fork@latest",
    "graphql": "15.8.0",
    "graphql-tag": "^2.12.6",
    "iterall": "^1.3.0",
    "lodash": "^4.17.21",
    "normalize-path": "^3.0.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-fastify": "^7.3.2",
    "class-transformer": "^0.5.0",
    "graphql": "^15.5.1",
    "reflect-metadata": "^0.1.13",
    "ts-morph": "^11.0.3 || ^12.0.0 || ^13.0.0"
  },
  "peerDependenciesMeta": {
    "ts-morph": {
      "optional": true
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mirco312312/nest-fgql.git"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testEnvironment": "node",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  }
}